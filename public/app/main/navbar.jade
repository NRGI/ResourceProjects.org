.navbar.navbar-fixed-top.navbar-inner(ng-controller="nrgiNavBarCtrl")
    .container-fluid
        a.btn.btn-navbar(
        data-toggle='collapse'
        data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
        .col-md-3
            a.brand(href='/')
                img(
                src='../assets/rp-logo-2.png'
                width='185'
                alt='Natural Resource Governance Institute Logo')
        .col-md-9
            .nav-collapse
                //div(ng-include="'/partials/account/navbar'")
                .navbar-right(ng-controller="nrgiNavBarCtrl")
                    .col-md-7
                        ul.nav.list-inline.left-padding-lg
                            li.nav-item
                                a.nav-link(href='/projects') Projects
                            //li
                            //    a.dropdown-toggle(data-toggle="dropdown") Projects
                            //        b.caret
                            //    ul.dropdown-menu
                            //        li
                            //            a(href='/projects') Projects
                            //li
                            //    a(href='/projects') Payments
                            li.nav-item
                                a.nav-link(href='/projects/map') Map
                            li.nav-item
                                a.nav-link.dropdown-toggle(data-toggle="dropdown") Other Data
                                    b.caret
                                ul.dropdown-menu
                                    li
                                        a(href='/sites').dropdown-item Sites
                                    li
                                        a(href='/fields').dropdown-item Fields
                                    li
                                        a(href='/concessions').dropdown-item Concessions
                                    li
                                        a(href='/companies').dropdown-item Companies
                                    li
                                        a(href='/groups').dropdown-item Company Groups
                                    li
                                        a(href='/contracts').dropdown-item Contracts
                                    li
                                        a(href='/countries').dropdown-item Countries
                                    li
                                        a(href='/source_types').dropdown-item Source types
                                    //li
                                    //    a(href='/commodities').dropdown-item Commodities
                            li.nav-item
                                a.nav-link.dropdown-toggle(data-toggle="dropdown") About
                                    b.caret
                                ul.dropdown-menu
                                    li
                                        a(href='/about').dropdown-item About
                                    //li
                                    //    a(href='/sources').dropdown-item Sources
                                    li
                                        a(href='/glossary').dropdown-item Glossary
                            li.nav-item(ng-hide="!identity.isAuthenticated()")
                                a.nav-link.dropdown-toggle(data-toggle="dropdown") Admin
                                    b.caret
                                ul.dropdown-menu
                                    li
                                        a(href='/admin/etl/datasets').dropdown-item Manage Data Imports
                                    li
                                        a(href='/admin/reconcile').dropdown-item Reconcile Data
                                    li
                                        a(href='/admin/user-admin').dropdown-item Manage Users
                                    li
                                        a(href='/admin/sourceType-admin').dropdown-item Manage Source Types
                                    //li
