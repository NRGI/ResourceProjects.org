                                    //    a(href='/admin/create-commodity').dropdown-item Create Commodity
                                    //li
                                    //    a(href='/admin/create-company').dropdown-item Create Company
                                    //li
                                    //    a(href='/admin/create-group').dropdown-item Create Group

                    .col-md-5
                        form.navbar-search.pull-right(action='')
                            input.typeahead.search-query.span2(type='text' data-provide='typeahead' placeholder='Search')
                            a.glyphicon.text-primary.glyphicon-log-out.left-padding-med(
                            ng-hide="!identity.isAuthenticated()"
                            href=""
                            ng-click="signout()"
                            title="Logout")




