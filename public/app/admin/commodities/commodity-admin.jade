.container.top-padding-med
	.well
		legend Commodities
		.pull-right
			.btn-group
				button.btn.btn-default.pull-right(type='button')
					a(href='/admin/create-commodity') Create new commodity
		table.table
			thead
				tr
					th.tablesort-sortable-th Name
					th
			tbody
				tr(ng-repeat="commodity in commodities | unique: '_id'")
					td.commodity_name
						span(ng-bind-html="commodity.commodity_name|sanitize")
					td
						a.btn.center-block.btn-default(href="/admin/commodity-admin/{{commodity._id}}") Edit