.navbar-header a.navbar-brand
  padding-top 30px
  margin-top -15px
  margin-bottom -20px
  h6
    color #ccc

.navbar-brand span
  display block
  color rgba(64, 64, 64, 0.7)

.navbar-brand, .navbar-brand:hover
  font-size 19px
  font-family Open-Sans-Bold
  color #F37356
  text-transform uppercase
  padding 16px 20px 12px 0
  height auto
  width 1000px
  position relative

.navbar-brand .beta, .sidebar-brand .beta
  position absolute;
  font-size 12px;
  right -4px;
  top 13px;
  color #404040;
  color rgba(0, 0, 0, 0.5);
  text-transform none;
  font-family Open-Sans-Regular

.navbar-form
  .form-group
    margin-left 10px
  button
    margin-left 10px

.navbar .nav
  position relative
  left 0
  display block
  float left
  margin 0 10px 0 0

.navbar .brand
  display block
  float left
  padding 8px 8px 12px
  margin-left -20px
  font-size 20px
  font-weight 200
  line-height 1
  color #999

.navbar .brand img
  width 12vw

.navbar-inner
  min-height 40px
  padding-right 20px
  padding-left 20px
  background-color #fff
  background-image none
  background-repeat none
  -webkit-border-radius 4px
  -moz-border-radius 4px
  border-radius 4px
  -webkit-box-shadow none
  -moz-box-shadow none
  box-shadow none

.nav-item
  position relative
  display inline-block !important

.navbar .nav-link
  float none
  padding 10px .35vw
  line-height 19px
  color rgb(85,114,136)
  text-decoration none
  text-shadow none
  //font-size 1em
  font-size .9vw

.navbar .nav-link:hover
  color:rgb(255,111,76)
  text-decoration none
  background-color #fff !important

.navbar-fixed-top
  /*border-top 25px solid #B0D6F2!important;*/
  /*border-bottom 1px rgb(85,114,136) solid;*/
  border-bottom 1px solid #EEEFE8 !important
  z-index: 99999

.navbar-search .search-query
  padding: 6px 5px;
  font-size: 13px;
  font-weight: normal;
  line-height: 1;
  border-radius: 20px;
  border: 1px solid #151515;
  -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,0.1), 0 1px 0 rgba(255,255,255,0.15);
  -moz-box-shadow: inset 0 1px 2px rgba(0,0,0,0.1), 0 1px 0 rgba(255,255,255,0.15);
  box-shadow: inset 0 1px 2px rgba(0,0,0,0.1), 0 1px 0 rgba(255,255,255,0.15);
  -webkit-transition: none;
  -moz-transition: none;
  -ms-transition: none;
  -o-transition: none;
  transition: none;

.navbar-search
  //padding-right: 0px;
  position: relative;
  float: left;
  margin-top: 6px;
  margin-bottom: 0;

.navlogout
  padding-left .5vw
  font-size 1.2vw


.typeahead>li>a
  word-wrap break-word
  white-space normal